@model IEnumerable<FrontEnd.Models.MazoViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MazoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreMazo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreadoEn)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MazoId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NombreMazo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UsuarioId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreadoEn)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.MazoId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.MazoId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.MazoId })
                </td>
            </tr>
        }
    </tbody>
</table>
